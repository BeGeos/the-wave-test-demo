{
  "name": "the-wave-api-demo",
  "version": "1.0.0",
  "description": "Test api",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "start": "nodemon build/server.js",
    "start:dev": "pnpm run start",
    "start:prod": "pnpm run dz:migrate && node build/server.js",
    "dz:generate": "drizzle-kit generate:pg --config=drizzle.config.json",
    "dz:drop": "drizzle-kit drop",
    "dz:push": "drizzle-kit push:pg --config=drizzle.config.json",
    "dz:migrate": "node build/db/migrate.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "the wave",
    "rick & morty"
  ],
  "author": "BeGeos",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "drizzle-kit": "^0.20.14",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  }
}
